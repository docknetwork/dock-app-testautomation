<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Wallet Tests" parallel="tests"
       thread-count="1"
       configfailurepolicy="continue" verbose="1">
    <listeners>
        <listener
                class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener
                class-name="org.uncommons.reportng.JUnitXMLReporter"/>
    </listeners>

    <test name="Wallet Tests After App Reset- Android">
        <classes>
            <class name="dock.android.smoketests.appResetTrue.BackupWallet"></class>
            <class name="dock.android.smoketests.appResetFalse.ExportAccountAsJson"></class>
            <class name="dock.android.smoketests.appResetFalse.ExportDID"></class>
        </classes>
    </test>
</suite>